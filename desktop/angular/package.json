    "@angular/compiler-cli": "^16.0.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@types/chrome": "^0.0.236",
    "@types/d3": "^7.4.0",
    "@types/data-urls": "^3.0.4",
